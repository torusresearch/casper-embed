{
  "name": "@toruslabs/casper-embed",
  "version": "4.0.0",
  "description": "Embed script for casper blockchain",
  "main": "dist/torus.cjs.js",
  "module": "dist/torus.esm.js",
  "unpkg": "dist/torus.umd.min.js",
  "jsdelivr": "dist/torus.umd.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "torus-scripts start --name=torus",
    "build": "torus-scripts build --name=torus",
    "release": "torus-scripts release",
    "prepack": "npm run build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torusresearch/casper-embed.git"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@metamask/rpc-errors": "^6.0.0",
    "@toruslabs/base-controllers": "^4.0.0",
    "@toruslabs/http-helpers": "^5.0.0",
    "@toruslabs/openlogin-jrpc": "^5.0.2",
    "fast-deep-equal": "^3.1.3",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.22.15",
    "@toruslabs/config": "^2.0.2",
    "@toruslabs/eslint-config-typescript": "^3.0.1",
    "@toruslabs/torus-scripts": "^5.0.4",
    "@types/node": "^20",
    "@types/once": "^1.4.1",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^4.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "Casper",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torusresearch/casper-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/casper-embed#readme",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  }
}
